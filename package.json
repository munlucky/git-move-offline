{
  "name": "git-move-offline",
  "version": "1.0.4",
  "description": "Git 저장소를 히스토리와 함께 오프라인 환경으로 이동 및 동기화하는 도구",
  "bin": {
    "gitmv": "bin/gitmv.js"
  },
  "scripts": {
    "export": "node export.js",
    "import": "node import.js",
    "pack": "npm pack",
    "test-install": "npm pack && npm install -g git-move-offline-*.tgz",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "files": [
    "bin/",
    "lib/",
    "export.js",
    "import.js",
    "README.md",
    "USAGE.md",
    "config.example.json"
  ],
  "keywords": [
    "git",
    "offline",
    "bundle",
    "migration",
    "sync",
    "cli"
  ],
  "author": "moonsg <munlucky@naver.com>",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/munlucky/git-move-offline.git"
  },
  "bugs": {
    "url": "https://github.com/munlucky/git-move-offline/issues"
  },
  "homepage": "https://github.com/munlucky/git-move-offline#readme",
  "dependencies": {
    "archiver": "^7.0.1",
    "extract-zip": "^2.0.1",
    "inquirer": "^8.0.0"
  },
  "devDependencies": {
    "prettier": "^3.6.2"
  }
}
